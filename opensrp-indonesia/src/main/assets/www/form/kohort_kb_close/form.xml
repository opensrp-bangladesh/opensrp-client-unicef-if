<form autocomplete="off" class="or clearfix" id="Penutupan_KB" novalidate="novalidate">
    <!--This form was created by transforming a OpenRosa-flavored (X)Form using an XSLT sheet created by Enketo LLC.-->
    <section class="form-logo"></section>
    <h3 id="form-title">Penutupan KB</h3>
    <select data-default-lang="Bahasa" id="form-languages">
        <option value="Bahasa">Bahasa</option>
        <option value="en">English</option>
    </select>


    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason:label"
                    lang="Bahasa">Alasan menutup KB
                </span>
                <span class="question-label " data-itext-id="/penutupan_kb_reviewed/kb_close_reason:label"
                    lang="en">Reason for Family
                    Planning's Closure
                </span>
                <span class="required">*</span>
                <span class="or-hint active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason:hint"
                    lang="Bahasa">Pilihlah salah satu
                    alasan yang tersedia
                </span>
                <span class="or-hint " data-itext-id="/penutupan_kb_reviewed/kb_close_reason:hint"
                    lang="en">Select one of
                    reasons available
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/penutupan_kb_reviewed/kb_close_reason" data-name="/penutupan_kb_reviewed/kb_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="death_of_mother" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason/death_of_mother:label"
                        lang="Bahasa">
                        Ibu meninggal
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/kb_close_reason/death_of_mother:label"
                        lang="en">
                        Maternal death
                    </span>
                </label>
                <label class="">
                    <input name="/penutupan_kb_reviewed/kb_close_reason" data-name="/penutupan_kb_reviewed/kb_close_reason"
                        data-type-xml="select1"
                        required="required" type="radio" value="permanent_relocation" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason/permanent_relocation:label"
                        lang="Bahasa">
                        Relokasi
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/kb_close_reason/permanent_relocation:label"
                        lang="en">
                        Relocation
                    </span>
                </label>
                <label class="">
                    <input name="/penutupan_kb_reviewed/kb_close_reason" data-name="/penutupan_kb_reviewed/kb_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="dropout" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason/dropout:label"
                        lang="Bahasa">Drop
                        Out (DO)
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/kb_close_reason/dropout:label"
                        lang="en">Drop
                        Out (DO)
                    </span>
                </label>
                <label class="">
                    <input name="/penutupan_kb_reviewed/kb_close_reason" data-name="/penutupan_kb_reviewed/kb_close_reason"
                        data-type-xml="select1" required="required"
                        type="radio" value="gagal" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/kb_close_reason/gagal:label"
                        lang="Bahasa">Gagal (G)
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/kb_close_reason/gagal:label"
                        lang="en">Failure
                        (F)
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-branch pre-init non-select ">
        <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_do:label"
            lang="Bahasa">Keterangan DO
        </span>
        <span class="question-label " data-itext-id="/penutupan_kb_reviewed/keterangan_do:label"
            lang="en">Note of Drop Out
        </span>
        <span class="or-hint active" data-itext-id="/penutupan_kb_reviewed/keterangan_do:hint"
            lang="Bahasa">Berikan alasan dan penjelasan
            penyebab peserta KB Drop Out
        </span>
        <span class="or-hint " data-itext-id="/penutupan_kb_reviewed/keterangan_do:hint" lang="en">
            Write the reason for Drop Out
        </span>
        <input name="/penutupan_kb_reviewed/keterangan_do" data-relevant=" /penutupan_kb_reviewed/kb_close_reason  = 'dropout'"
            data-type-xml="string"
            type="text" />
    </label>
    <fieldset class="question or-branch pre-init ">
        <fieldset>
            <legend>
                <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal:label"
                    lang="Bahasa">Keterangan Gagal
                </span>
                <span class="question-label " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal:label"
                    lang="en">Note of Failure
                </span>
                <span class="or-hint active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal:hint"
                    lang="Bahasa">Berikan alasan dan
                    penjelasan tambahan berkaitan peserta gagal KB
                </span>
                <span class="or-hint " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal:hint"
                    lang="en">Write the reason
                    for contraceptive failure
                </span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/penutupan_kb_reviewed/keterangan_gagal" data-name="/penutupan_kb_reviewed/keterangan_gagal"
                        data-relevant=" /penutupan_kb_reviewed/kb_close_reason  = 'gagal'" data-type-xml="select1"
                        type="radio"
                        value="ibu_hamil" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/ibu_hamil:label"
                        lang="Bahasa">Ibu
                        hamil
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/ibu_hamil:label"
                        lang="en">
                        Mother becomes pregnant
                    </span>
                </label>
                <label class="">
                    <input name="/penutupan_kb_reviewed/keterangan_gagal" data-name="/penutupan_kb_reviewed/keterangan_gagal"
                        data-relevant=" /penutupan_kb_reviewed/kb_close_reason  = 'gagal'"
                        data-type-xml="select1"
                        type="radio"
                        value="tidak_ada_keterangan" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/tidak_ada_keterangan:label"
                        lang="Bahasa">
                        Tidak ada keterangan
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/tidak_ada_keterangan:label"
                        lang="en">
                        None
                    </span>
                </label>
                <label class="">
                    <input name="/penutupan_kb_reviewed/keterangan_gagal" data-name="/penutupan_kb_reviewed/keterangan_gagal"
                        data-relevant=" /penutupan_kb_reviewed/kb_close_reason  = 'gagal'" data-type-xml="select1"
                        type="radio"
                        value="lainnya" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/lainnya:label"
                        lang="Bahasa">
                        Lainnya
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal/lainnya:label"
                        lang="en">Other
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>
    <label class="question or-branch pre-init non-select ">
        <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/keterangan_gagal_lainnya:label"
            lang="Bahasa">Keterangan gagal
            lainnya
        </span>
        <span class="question-label " data-itext-id="/penutupan_kb_reviewed/keterangan_gagal_lainnya:label"
            lang="en">Other note of
            failure
        </span>
        <input name="/penutupan_kb_reviewed/keterangan_gagal_lainnya" data-relevant=" /penutupan_kb_reviewed/keterangan_gagal  = 'lainnya'"
            data-type-xml="string"
            type="text" />
    </label>
    <label class="question or-branch pre-init non-select ">
        <span class="or-constraint-msg active" data-itext-id="/penutupan_kb_reviewed/death_date:jr:constraintMsg"
            lang="Bahasa">Tanggal kematian ibu
            hari ini atau hari-hari sebelumnya
        </span>
        <span class="or-constraint-msg " data-itext-id="/penutupan_kb_reviewed/death_date:jr:constraintMsg"
            lang="en">Date of mother's
            death today or the days before
        </span>
        <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/death_date:label"
            lang="Bahasa">Tanggal kematian
        </span>
        <span class="question-label " data-itext-id="/penutupan_kb_reviewed/death_date:label"
            lang="en">Date of death
        </span>
        <span class="or-hint active" data-itext-id="/penutupan_kb_reviewed/death_date:hint"
            lang="Bahasa">masukkan tanggal kematian
        </span>
        <span class="or-hint " data-itext-id="/penutupan_kb_reviewed/death_date:hint" lang="en">
            Enter the date of death
        </span>
        <input name="/penutupan_kb_reviewed/death_date" data-constraint=".&lt;= /penutupan_kb_reviewed/today "
            data-relevant=" /penutupan_kb_reviewed/kb_close_reason  = 'death_of_mother'"
            data-type-xml="date"
            type="date" />
    </label>
    <fieldset class="question ">
        <fieldset>
            <legend>
                <span class="question-label active" data-itext-id="/penutupan_kb_reviewed/confirm_kb_close:label"
                    lang="Bahasa">konfirmasi
                    penutupan KB?
                </span>
                <span class="question-label " data-itext-id="/penutupan_kb_reviewed/confirm_kb_close:label"
                    lang="en">Confirmation of
                    Family Planning Closure?
                </span>
                <span class="required">*</span>
            </legend>
            <div class="option-wrapper">
                <label class="">
                    <input name="/penutupan_kb_reviewed/confirm_kb_close" data-name="/penutupan_kb_reviewed/confirm_kb_close"
                        data-type-xml="select1" required="required"
                        type="radio" value="yes" />
                    <span class="option-label active" data-itext-id="/penutupan_kb_reviewed/confirm_kb_close/yes:label"
                        lang="Bahasa">Ya
                    </span>
                    <span class="option-label " data-itext-id="/penutupan_kb_reviewed/confirm_kb_close/yes:label"
                        lang="en">Yes
                    </span>
                </label>
            </div>
        </fieldset>
    </fieldset>

    <fieldset style="display:none;" id="or-preload-items">
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/start" data-preload="timestamp" data-preload-params="start"
                data-type-xml="dateTime" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/today" data-preload="date" data-preload-params="today"
                data-type-xml="date" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/deviceid" data-preload="property" data-preload-params="deviceid"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/simserial" data-preload="property" data-preload-params="simserial"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/phonenumber" data-preload="property" data-preload-params="phonenumber"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/end" data-preload="timestamp" data-preload-params="end"
                data-type-xml="dateTime" type="hidden" />
        </label>
    </fieldset>
    <fieldset style="display:none;" id="or-calculated-items">
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/meta/instanceID" data-calculate="concat('uuid:', uuid())"
                data-type-xml="string" type="hidden" />
        </label>
        <label class="calculation non-select ">
            <input name="/penutupan_kb_reviewed/formhub/uuid" data-calculate="'658791259ff5480a9dfc86e5955defa5'"
                data-type-xml="string" type="hidden" />
        </label>
    </fieldset>
</form>